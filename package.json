    "supertest": "^2.0.1"
  },
  "dependencies": {
    "algoliasearch": "^3.22.1",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "connect-ensure-login": "^0.1.1",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "createsend-node": "^0.8.2",
